func convertStenoNumMainRight()
{
   if (mainRight == 0) { stenoNumMainRight = 0; return; }
   var stenoNum = 1;
   if (mainRight == _E) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == _W) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == _D) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == _S) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == _F) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_E|_W)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_S|_D)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_D|_F)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_A)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_D|_S|_F)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
   if (mainRight == (_R)) { stenoNumMainRight = stenoNum; return; } stenoNum = stenoNum + 1;
