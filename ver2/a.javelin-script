func pressStenoLeft() {
   if (stenoNumMainLeft & 1) { pressStenoKey(SK_DR); }
   if (stenoNumMainLeft & 2) { pressStenoKey(SK_S1); }
   if (stenoNumMainLeft & 4) { pressStenoKey(SK_TL); }
   if (stenoNumMainLeft & 8) { pressStenoKey(SK_KL); }
   if (stenoNumMainLeft & 16) { pressStenoKey(SK_PL); }
   if (stenoNumMainLeft & 32) { pressStenoKey(SK_WL); }
   if (stenoNumMainLeft & 64) { pressStenoKey(SK_HL); }
   if (stenoNumMainLeft & 128) { pressStenoKey(SK_RL); }
}

func releaseStenoLeft() {
   if (stenoNumMainLeft & 1) { releaseStenoKey(SK_DR); }
   if (stenoNumMainLeft & 2) { releaseStenoKey(SK_S1); }
   if (stenoNumMainLeft & 4) { releaseStenoKey(SK_TL); }
   if (stenoNumMainLeft & 8) { releaseStenoKey(SK_KL); }
   if (stenoNumMainLeft & 16) { releaseStenoKey(SK_PL); }
   if (stenoNumMainLeft & 32) { releaseStenoKey(SK_WL); }
   if (stenoNumMainLeft & 64) { releaseStenoKey(SK_HL); }
   if (stenoNumMainLeft & 128) { releaseStenoKey(SK_RL); }
}

func pressStenoRight() {
   if (stenoNumMainRight & 1) { pressStenoKey(SK_FR); }
   if (stenoNumMainRight & 2) { pressStenoKey(SK_RR); }
   if (stenoNumMainRight & 4) { pressStenoKey(SK_PR); }
   if (stenoNumMainRight & 8) { pressStenoKey(SK_BR); }
   if (stenoNumMainRight & 16) { pressStenoKey(SK_LR); }
   if (stenoNumMainRight & 32) { pressStenoKey(SK_GR); }
   if (stenoNumMainRight & 64) { pressStenoKey(SK_TR); }
}

func releaseStenoRight() {
   if (stenoNumMainRight & 1) { releaseStenoKey(SK_FR); }
   if (stenoNumMainRight & 2) { releaseStenoKey(SK_RR); }
   if (stenoNumMainRight & 4) { releaseStenoKey(SK_PR); }
   if (stenoNumMainRight & 8) { releaseStenoKey(SK_BR); }
   if (stenoNumMainRight & 16) { releaseStenoKey(SK_LR); }
   if (stenoNumMainRight & 32) { releaseStenoKey(SK_GR); }
   if (stenoNumMainRight & 64) { releaseStenoKey(SK_TR); }
}

func pressStenoSubLeft()
{
   if (subLeft == 0) { return; }
   if (subLeft == (RA)) { pressStenoKey(SK_A); return; }
   if (subLeft == (RO)) { pressStenoKey(SK_O); return; }
   if (subLeft == (RA|RO)) { pressStenoKey(SK_A); pressStenoKey(SK_O); return; }
   if (subLeft == (R4)) { pressStenoKey(SK_SR); pressStenoKey(SK_A); pressStenoKey(SK_O); return; }
   if (subLeft == (R4|RA)) { pressStenoKey(SK_A); pressStenoKey(SK_SR); return; }
   if (subLeft == (V|RA)) { pressStenoKey(SK_O); pressStenoKey(SK_SR); return; }
   if (subLeft == (V)) { pressStenoKey(SK_SR); return; }
}

func pressStenoSubRight()
{
   if (subRight == 0) { return; }
   if (subRight == (_RA)) { pressStenoKey(SK_U); return; }
   if (subRight == (_RO)) { pressStenoKey(SK_E); return; }
   if (subRight == (_RA|_RO)) { pressStenoKey(SK_U); pressStenoKey(SK_E); return; }
   if (subRight == (_R4)) { pressStenoKey(SK_ZR); pressStenoKey(SK_U); pressStenoKey(SK_E); return; }
   if (subRight == (_R4|_RA)) { pressStenoKey(SK_U); pressStenoKey(SK_ZR); return; }
   if (subRight == (_V|_RA)) { pressStenoKey(SK_E); pressStenoKey(SK_ZR); return; }
   if (subRight == (_V)) { pressStenoKey(SK_ZR); return; }
}

func releaseStenoSubLeft()
{
   if (subLeft == 0) { return; }
   if (subLeft == (RA)) { releaseStenoKey(SK_A); return; }
   if (subLeft == (RO)) { releaseStenoKey(SK_O); return; }
   if (subLeft == (RA|RO)) { releaseStenoKey(SK_A); releaseStenoKey(SK_O); return; }
   if (subLeft == (R4)) { releaseStenoKey(SK_SR); releaseStenoKey(SK_A); releaseStenoKey(SK_O); return; }
   if (subLeft == (R4|RA)) { releaseStenoKey(SK_A); releaseStenoKey(SK_SR); return; }
   if (subLeft == (V|RA)) { releaseStenoKey(SK_O); releaseStenoKey(SK_SR); return; }
   if (subLeft == (V)) { releaseStenoKey(SK_SR); return; }
}

func releaseStenoSubRight()
{
   if (subRight == 0) { return; }
   if (subRight == (_RA)) { releaseStenoKey(SK_U); return; }
   if (subRight == (_RO)) { releaseStenoKey(SK_E); return; }
   if (subRight == (_RA|_RO)) { releaseStenoKey(SK_U); releaseStenoKey(SK_E); return; }
   if (subRight == (_R4)) { releaseStenoKey(SK_ZR); releaseStenoKey(SK_U); releaseStenoKey(SK_E); return; }
   if (subRight == (_R4|_RA)) { releaseStenoKey(SK_U); releaseStenoKey(SK_ZR); return; }
   if (subRight == (_V|_RA)) { releaseStenoKey(SK_E); releaseStenoKey(SK_ZR); return; }
   if (subRight == (_V)) { releaseStenoKey(SK_ZR); return; }
}

func resetAllStates() {
   mainLeft = 0;
   mainRight = 0;
   subLeft = 0;
   subRight = 0;
   stenoNumMainLeft = 0;
   stenoNumMainRight = 0;
}

func onPress0() { mainLeft = mainLeft | R1; } func onRelease0() { sendSteno(); }
func onPress1() { mainLeft = mainLeft | Q; } func onRelease1() { sendSteno(); }
func onPress2() { mainLeft = mainLeft | W; } func onRelease2() { sendSteno(); }
func onPress3() { mainLeft = mainLeft | E; } func onRelease3() { sendSteno(); }
func onPress4() { mainLeft = mainLeft | R; } func onRelease4() { sendSteno(); }
func onPress5() { mainLeft = mainLeft | T; } func onRelease5() { sendSteno(); }
func onPress6() { mainRight = mainRight | _T; } func onRelease6() { sendSteno(); }
func onPress7() { mainRight = mainRight | _R; } func onRelease7() { sendSteno(); }
func onPress8() { mainRight = mainRight | _E; } func onRelease8() { sendSteno(); }
func onPress9() { mainRight = mainRight | _W; } func onRelease9() { sendSteno(); }
func onPress10() { mainRight = mainRight | _Q; } func onRelease10() { sendSteno(); }
func onPress11() { mainRight = mainRight | _R1; } func onRelease11() { sendSteno(); }
func onPress12() { mainLeft = mainLeft | R2; } func onRelease12() { sendSteno(); }
